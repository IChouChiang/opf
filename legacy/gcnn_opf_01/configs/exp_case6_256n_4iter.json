{
    "model": {
        "n_bus": 6,
        "n_gen": 3,
        "feature_iterations": 4,
        "channels_gc_out": 8,
        "neurons_fc": 256,
        "n_gc_layers": 2,
        "n_fc_layers": 1,
        "activation_gs": "tanh",
        "model_type": "flattened"
    },
    "training": {
        "optimizer": "adam",
        "batch_size": 6,
        "learning_rate": 0.001,
        "epochs": 50,
        "early_stopping_patience": 10,
        "weight_decay": 1e-5,
        "kappa": 0.1,
        "use_physics_loss": true,
        "two_stage": true,
        "phase1_epochs": 25,
        "phase2_epochs": 25,
        "phase2_lr": 1e-4,
        "phase2_kappa": 1.0
    }
}
